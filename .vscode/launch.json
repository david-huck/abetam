    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit app",
            "type": "python",
            "request": "launch",
            "module": "streamlit",
            "justMyCode": true,
            "args": ["run", "${file}"]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "model.py as Module",
            "type": "python",
            "request": "launch",
            "module": "components.model",
            "console": "integratedTerminal",
            "justMyCode": true,
            // "args": ["-m"]
        }
    ]
}